		System.out.println("Digite o primeiro coeficiente da equacao de segundo grau: ");
		a = scanner.nextInt();
		System.out.println("Digite o segundo coeficiente da equacao de segundo grau: ");
		b = scanner.nextInt();
		System.out.println("Digite o terceiro coeficiente da equacao de segundo grau: ");
		c = scanner.nextInt();
		
		delta = (b*b)-(4*a*c);
		
		System.out.println(delta);
		
