package Slide21;

public class Pessoa {
	String nome;
	int idade;
	double altura;
	
	public Pessoa(String nome, int idade, double altura) {
		this.nome = nome;
		this.idade = idade;
		this.altura = altura;
	}
	
	public apresentar() {
		System.out.println(this.nome + " tem " + this.idade + " e mede " + this.altura);
	}

}
